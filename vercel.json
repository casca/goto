{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "(/(?!goto)(?!api).*)",
      "destination": "/goto"
    },
    {
      "source": "/goto/api/(.*)",
      "destination": "https://goto.casca.vercel.app/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/goto/((?!api/).*)",
      "destination": "https://goto.casca.vercel.app/goto"
    }
  ]
}
